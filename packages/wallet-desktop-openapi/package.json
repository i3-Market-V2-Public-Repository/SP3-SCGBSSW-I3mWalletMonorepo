{
  "name": "@i3-market/wallet-desktop-openapi",
  "version": "0.9.0",
  "description": "The API of the i3m-wallet desktop application",
  "main": "openapi.json",
  "exports": {
    ".": "./openapi.json",
    "./types": "./types/index.d.ts",
    "./openapi.yaml": "./openapi.yaml",
    "./openapi.json": "./openapi.json"
  },
  "directories": {
    "types": "./types",
    "src": "./src",
    "build": "./build"
  },
  "scripts": {
    "build": "run-s bundle types",
    "bundle": "ts-node build/createOASJsonBundle.ts",
    "commit": "git commit -a -m \"$npm_package_version\"",
    "types": "ts-node build/openApiTypes.ts",
    "lint": "ts-standard --fix",
    "postbuild": "npm run lint",
    "prepublishOnly": "npm run build",
    "postversion": "run-s build commit"
  },
  "publishConfig": {
    "@i3-market:registry": "https://gitlab.com/api/v4/projects/26002446/packages/npm/"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:i3-market/code/wp3/t3.2/i3m-wallet/wallet-desktop-openapi.git"
  },
  "keywords": [],
  "author": "Juan Hern√°ndez Serrano",
  "license": "ISC",
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@dtsgenerator/replace-namespace": "^1.4.1",
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "chalk": "^4.1.0",
    "dtsgenerator": "^3.8.0",
    "glob": "^7.1.6",
    "jose": "^3.11.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "openapi-types": "^8.0.0",
    "ts-node": "^9.1.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
